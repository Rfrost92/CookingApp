{
  "cli": {
    "version": ">= 10.0.3"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "firebaseApiKey": "$(firebaseApiKey)",
        "runwareApiKey": "$(runwareApiKey)",
        "googleWebClientId": "$(googleWebClientId)",
        "gptApiKey": "$(gptApiKey)"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "firebaseApiKey": "$(firebaseApiKey)",
        "runwareApiKey": "$(runwareApiKey)",
        "googleWebClientId": "$(googleWebClientId)",
        "gptApiKey": "$(gptApiKey)"
      }
    },
    "production": {
      "env": {
        "firebaseApiKey": "$(firebaseApiKey)",
        "runwareApiKey": "$(runwareApiKey)",
        "googleWebClientId": "$(googleWebClientId)",
        "gptApiKey": "$(gptApiKey)"
      }
    }
  },
  "submit": {
    "production": {
      "env": {
        "firebaseApiKey": "$(firebaseApiKey)",
        "runwareApiKey": "$(runwareApiKey)",
        "googleWebClientId": "$(googleWebClientId)",
        "gptApiKey": "$(gptApiKey)"
      }
    }
  }
}
