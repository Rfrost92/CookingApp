{
  "expo": {
    "name": "Smart Chef",
    "slug": "SmartChef",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./App.tsx",
    "icon": "./assets/SmartChefLogo2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "smartchef",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rFrost.SmartChef",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rFrost.SmartChef"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3d61730c-3eff-4a01-8439-bc1a1ac4aedf"
      }
    },
    "plugins": [
      "@react-native-firebase/auth",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "react-native-iap",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "roman-moroz"
        }
      ]
    ]
  }
}
